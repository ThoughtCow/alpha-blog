<% @head = "Articles - " %>
<h1 class="arial">All articles</h1>
<a href="https://maya-dog-walking.herokuapp.com/details" class="button arial">Create a new article</a>
<p> </p>
<table>
  <thead>
    <tr>
      <th>Article</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.created_at %></td>
      <td><p>placeholder</p></td>
    </tr>
    <% end %>
  </tbody>
</table>
